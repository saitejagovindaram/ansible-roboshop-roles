proxy_http_version 1.1;
location /images/ {
  expires 5s;
  root   /usr/share/nginx/html;
  try_files $uri /images/placeholder.jpg;
}
location /api/catalogue/ { proxy_pass {{CATALOGUE_URL}}:8080/; }
location /api/user/ { proxy_pass {{USER_URL}}:8080/; }
location /api/cart/ { proxy_pass {{CART_URL}}; }
location /api/shipping/ { proxy_pass {{SHIPPING_URL}}; }
location /api/payment/ { proxy_pass {{PAYMENTS_URL}}; }

location /health {
  stub_status on;
  access_log off;
}